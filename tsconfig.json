{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2021", "DOM"],
    "module": "CommonJS",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "allowJs": true,
    "noEmit": true,
    "types": ["cypress", "node", "@badeball/cypress-cucumber-preprocessor"]
  },
  "include": ["cypress/**/*.ts", "cypress/**/*.js"]
}
